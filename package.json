{
  "name": "cypress",
  "version": "1.0.0",
  "description": "An E2E Test Automation framework",
  "main": "index.js",
  "scripts": {
    "openCypress": "node_modules/.bin/cypress open",
    "runHeadless": "node_modules/.bin/cypress run",
    "headTest": "npm run test -- --headed",
    "chromeTest": "npm run test -- --browser chrome",
    "dashboardRun": "npm run test --record --key b0e0ef56-b1b8-4d68-9c77-59d797de4d19 --browser chrome"
  },
  "keywords": [
    "automation"
  ],
  "author": "akash",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "cypress": "^9.0.0",
    "cypress-xpath": "^1.6.2",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "typescript": "^4.4.4"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/steps/**/"
  },
  "dependencies": {
    "cypress-cucumber-preprocessor": "^4.3.0"
  },
  "files": [
    "/integration/*",
    "/pages/*",
    "/support/*",
    "tsconfig.json"
  ]
}
